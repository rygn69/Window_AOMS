SET SYSMENU TO
SET SYSMENU AUTOMATIC

DEFINE PAD _msm_file OF _MSYSMENU PROMPT "\<File" COLOR SCHEME 3 ;
	NEGOTIATE  LEFT, NONE ;
	KEY ALT+F, "" ;
	MESSAGE "Creates, opens, saves, prints files or quits Visual FoxPro"
DEFINE PAD _33n13ppqb OF _MSYSMENU PROMPT "Main \<Menu" COLOR SCHEME 3 ;
	KEY ALT+M, ""
DEFINE PAD _msm_systm OF _MSYSMENU PROMPT "\<Help" COLOR SCHEME 3 ;
	KEY ALT+H, "" ;
	MESSAGE "Displays Help on Visual FoxPro"
ON PAD _msm_file OF _MSYSMENU ACTIVATE POPUP _mfile
ON PAD _33n13ppqb OF _MSYSMENU ACTIVATE POPUP mainmenu
ON PAD _msm_systm OF _MSYSMENU ACTIVATE POPUP _msystem

DEFINE POPUP _mfile MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF _mfile PROMPT "\<Close" ;
	SKIP FOR .T.
DEFINE BAR 2 OF _mfile PROMPT "\-"
DEFINE BAR 3 OF _mfile PROMPT "E\<xit" ;
	MESSAGE "Quits Visual FoxPro"
ON SELECTION BAR 1 OF _mfile goApp.ReleaseForm
ON SELECTION BAR 3 OF _mfile goApp.Release

DEFINE POPUP mainmenu MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF mainmenu PROMPT "160\<0" ;
	KEY CTRL+0, "CTRL+0"
DEFINE BAR 2 OF mainmenu PROMPT "1604 \<E"
DEFINE BAR 3 OF mainmenu PROMPT "1604 \<CF"
ON SELECTION BAR 1 OF mainmenu goApp.DoForm("mn1600.scx")
ON SELECTION BAR 2 OF mainmenu goApp.DoForm("mn1604e.scx")
ON SELECTION BAR 3 OF mainmenu goApp.DoForm("mn1604cf.scx")

DEFINE POPUP _msystem MARGIN RELATIVE SHADOW COLOR SCHEME 4
DEFINE BAR 1 OF _msystem PROMPT "\<About..."
ON SELECTION BAR 1 OF _msystem goApp.DoForm("About2.scx")
